/* pages/singer/index/index.wxss */
.searchContainer{
    padding: 0 20rpx;
}
.header{
    display: flex;
    height: 60rpx;
    line-height: 60rpx;
    padding: 10rpx 0;
}
.searchInput{
    position: relative;
    flex: 1;
    background: rgba(237, 237, 237, 1);
    border-radius: 30rpx;
}
.searchInput .iconfont{
    color: #818585;

}


.singerContainer .singerList{
    padding: 10rpx;
    border-radius: 30rpx;
    margin: 40rpx;
    box-shadow: 9rpx 9rpx 9rpx 9rpx #818585;
}
.singerContainer .list{
    position: relative;
    display: flex;
}
.singerContainer .singerList .img{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}
.singerContainer .singerList .name{
    line-height: 150rpx;
    margin-left: 20rpx;
    font-weight: 600;
    color: #676b6b;
}
.singerContainer .singerList .look{
    position: absolute;
    text-align: center;
    width: 120rpx;
    height: 45rpx;
    line-height: 45rpx;
    color: #d43c33;
    border: 3px solid #d43c33;
    border-radius: 30rpx;
    left: 510rpx;
    top: 50rpx;
    /* background-color: #d43c33; */
}